{
  "name": "cypress-cucumber-example",
  "version": "1.0.0",
  "description": "Sample implementation of Cypress using Cucumber",
  "scripts": {
    "test": "cypress run --spec \"**/*.feature\"",
    "test:all": "cypress run --spec \"**/*.feature\"",
    "test:specific": "cypress run --spec \"**/ProvideYourDetails.feature\"",
    "test:chrome": "cypress run --spec \"**/*.feature\" --browser chrome",
    "test:headed": "cypress run --spec \"**/*.feature\" --headed",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "report": "node cucumber-html-report.js"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreasneuber/cypress-cucumber-example.git"
  },
  "keywords": [
    "cypress",
    "cucumber",
    "testing"
  ],
  "author": "Andreas Neuber",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andreasneuber/cypress-cucumber-example/issues"
  },
  "homepage": "https://github.com/andreasneuber/cypress-cucumber-example#readme",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "24.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.3",
    "cypress": "^15.0.0",
    "multiple-cucumber-html-reporter": "^3.9.3"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/e2e/step_definitions/**/*.{js,ts}",
    "json": {
      "enabled": true,
      "output": "cypress/cucumber-json/{name}.cucumber.json",
      "formatter": "cucumber-json"
    }
  }
}
